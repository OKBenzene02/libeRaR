<h2>Search Authors</h2>
<form action="/authors" method="GET">
    <label for="name">Name</label>
    <input type="text" name="name" value="<%= searchAuthor.name %>">
    <button type="submit">Search</button>
</form>

<br>

<% authors.forEach((author) => { %>
    <div><%= author.name %></div>
<% }) %>